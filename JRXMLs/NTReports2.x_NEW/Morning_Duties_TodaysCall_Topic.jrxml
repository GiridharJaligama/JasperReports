	<parameter name="tM_1" class="java.lang.String"/>
	<parameter name="tM_2" class="java.lang.String"/>
	<parameter name="tM_3" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[select * from (
               SELECT count(incident0_.Incident_ID) AS "2nd Day Calls"
					FROM Incident incident0_
			   WHERE 1 = 1
					AND incident0_.itemType_ID = 1
					AND (
							incident0_.createdate = $P{tM_2}							
							OR incident0_.createdate =  $P{tM_1}							
						 )
					AND (
						    close_date IS NULL
							OR close_date > concat($P{tM_1},' 12:00:00')
					) ) as 2nd,  
		       (
				 SELECT count(incident0_.Incident_ID) AS "3rd Day Calls"
						FROM Incident incident0_
						WHERE 1 = 1
							AND incident0_.itemType_ID = 1
							AND (
								 incident0_.createdate = $P{tM_3}								 
								 OR incident0_.createdate = $P{tM_2}								 
							)
							 AND (
								  close_date IS NULL
								  OR close_date > concat($P{tM_2},' 12:00:00')
							)			
		      ) as 3rd, 
              (
					SELECT count(incident0_.Incident_ID) AS "4th Day Calls"
						FROM Incident incident0_
							WHERE 1 = 1
					        AND incident0_.itemType_ID = 1
							AND (
								 incident0_.createdate = $P{tM_3}								 
								 OR incident0_.createdate < $P{tM_3}
							)
							AND (
								 close_date IS NULL
								 OR close_date > concat($P{tM_3},' 12:00:00')
							)		
		      ) as 4th]]>
	</queryString>
