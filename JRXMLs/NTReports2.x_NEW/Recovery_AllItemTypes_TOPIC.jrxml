	<queryString language="SQL">
		<![CDATA[select s1.stationcode as ascode, 
       count(incident.incident_ID) as incidentCount,
	   date(incident.close_date) as CloseDate
from Incident incident, station s1
where incident.status_ID = 13 and s1.station_ID = incident.stationassigned_ID
and $X{IN, s1.stationcode,AssignedStation}
and $X{BETWEEN, incident.createdate, sdate,edate}
and $X{IN, s1.companycode_ID,CompanyID}
group by s1.stationcode,incident.close_date 
order by
 incident.close_date, s1.stationcode]]>
