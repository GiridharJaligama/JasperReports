	<parameter name="CompanyID" class="java.util.List"/>
	<queryString language="SQL">
		<![CDATA[select * from (
select distinct
    count(c.remote_claim_id) matches, 
	c.id, 
	c.remote_claim_id, 
	i.airlineIncidentId, 
	c.claimDate, 
	c.amountClaimed, 
	c.amountClaimedCurrency, 
	c.amountPaid, 
	c.amountPaidCurrency,
    c.statusId
from fsfile f, fsclaim c, fsincident i ,matchhistory h
where 1=1 
      and c.file_id = f.id  
	  and f.id = i.file_id 
      and f.id = h.file1_id
      and $X{IN, f.company_code,CompanyID} 
	  and $X{BETWEEN, date(c.claimDate), sdate,edate}
	group by c.remote_claim_id
union
       select 0 matches, c.id, 
	   c.remote_claim_id, 
	   i.airlineIncidentId, 
	   c.claimDate, 
	   c.amountClaimed, 
	   c.amountClaimedCurrency, 
	   c.amountPaid, 
	   c.amountPaidCurrency,
       c.statusId
    from fsfile f,
   	     fsclaim c, 
		 fsincident i 
where 1=1 and c.file_id = f.id  
          and f.id = i.file_id 
		  and $X{BETWEEN, date(c.claimDate), sdate,edate}
		  and $X{IN, f.company_code,CompanyID}
		  and f.id not in 
		  (select  h.file1_id from matchhistory h  where h.file1_id!=c.remote_claim_id)
         ) aa order by aa.remote_claim_id]]>
	</queryString>
