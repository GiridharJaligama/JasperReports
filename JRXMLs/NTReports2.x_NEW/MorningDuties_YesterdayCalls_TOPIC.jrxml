	<parameter name="tM_3" class="java.lang.String"/>
	<parameter name="tM_2" class="java.lang.String"/>
	<parameter name="tM_1" class="java.lang.String"/>
	<parameter name="tM_4" class="java.lang.String"/>
	<parameter name="tM_5" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[select * from
           ( SELECT count(incident0_.Incident_ID) AS "Y2ndDayCalls"
			  FROM Incident incident0_
			  WHERE 1 = 1
					AND incident0_.itemType_ID = 1
					AND (
						 incident0_.createdate = $P{tM_3}						 
						 OR incident0_.createdate = $P{tM_2}						 
					 )
					AND (
						 close_date IS NULL
						 OR close_date > concat($P{tM_1},' 12:00:00')
					)		
		    ) AS y2nd,
		  (
		    SELECT count(incident0_.Incident_ID) AS "Y3rdDayCalls"
				FROM Incident incident0_
				WHERE 1 = 1
				AND incident0_.itemType_ID = 1
				AND (
					 incident0_.createdate = $P{tM_4}					 
					 OR incident0_.createdate = $P{tM_5}
					 
				 )
				AND (
					 close_date IS NULL
					 OR close_date > concat($P{tM_2},' 12:00:00')
				)
		    ) AS Y3rd,
			(
			 SELECT count(incident0_.Incident_ID) AS "Y4thDayCalls"
			    FROM Incident incident0_
				WHERE 1 = 1
				AND incident0_.itemType_ID = 1
				AND (
					 incident0_.createdate = $P{tM_4}
					 
					 OR incident0_.createdate < $P{tM_4}
					)
				AND (
					close_date IS NULL
					OR close_date > concat($P{tM_3},' 12:00:00')
				)	
				
			) as Y4th,			
	        (
			 SELECT count(incident0_.Incident_ID) AS "Y2ndDayCompleted"
				FROM Incident incident0_
				WHERE 1 = 1
					AND incident0_.itemType_ID = 1
					AND (
						incident0_.createdate = $P{tM_4}
						
						OR incident0_.createdate = $P{tM_3}
						
				)
				AND (
					 incident0_.Incident_ID IN (
							SELECT twodaytask1_.incident_id
								FROM task twodaytask1_
								WHERE twodaytask1_.task_type = '2DAYTASK'
								AND twodaytask1_.status_ID = 85
				     )
			    ) 
			 )
			 as y2ndcompleted,			
			 (
				SELECT count(incident0_.Incident_ID) AS "Y3rdDayCompleted"
					FROM Incident incident0_
					WHERE 1 = 1
					AND incident0_.itemType_ID = 1
					AND (
							incident0_.createdate = $P{tM_4}
							
							OR incident0_.createdate = $P{tM_3}
							
						)
					AND (
							incident0_.Incident_ID IN (
								SELECT threedayta1_.incident_id
								FROM task threedayta1_
								WHERE threedayta1_.task_type = '3DAYTASK'
								AND threedayta1_.status_ID = 85
						)
			        )

		    ) as y3rdcompleted,
			(
			  SELECT count(incident0_.Incident_ID) AS "Y4thDayCompleted"
				FROM Incident incident0_
				WHERE 1 = 1
					AND incident0_.itemType_ID = 1
					AND (
						 incident0_.createdate = $P{tM_4}
						 
						OR incident0_.createdate < $P{tM_4}
					)
				 AND (
						incident0_.Incident_ID IN (
						SELECT fourdaytas1_.incident_id
							FROM task fourdaytas1_
							WHERE fourdaytas1_.task_type = '4DAYTASK'
							AND fourdaytas1_.status_ID = 85
						)
			      )
			)as y4thcompleted]]>
	</queryString>
